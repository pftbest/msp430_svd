<?xml version="1.0"?>
<drivers id="Cortex_R4">
	<isa Type="Cortex_R4" ProcID="0x75803400">
		 <driver file="..\..\DebugServer\bin\DataSnapshotDecoder.dll">
			<connectionType Type="DataSnapshotViewer"/>
		</driver>
	</isa>
	
	<isa Type="Cortex_R5" ProcID="0x75803400">
		 <driver file="..\..\DebugServer\bin\DataSnapshotDecoder.dll">
			<connectionType Type="DataSnapshotViewer"/>
		</driver>
	</isa>
	
	<driverProperties>
		<version number="1.0"/>
		<choicelist id="Endianness" type="string_list" must="true">
			<choice value="Little Endian"></choice>
			<choice value="Big Endian"></choice>
		</choicelist>
		<current>
			<cchoice key="Endianness" value="Little Endian"/>
		</current>
	</driverProperties>
</drivers>
