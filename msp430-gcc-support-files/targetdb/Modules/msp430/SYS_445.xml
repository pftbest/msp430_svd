            <bitenum id="JMBCLR1OFF_0" value="0x0" description="JMBIN1FG cleared on read of JMB1IN register"/>
            <bitenum id="JMBCLR1OFF_1" value="0x1" description="JMBIN1FG cleared by software"/>
        </bitfield>
    </register>
