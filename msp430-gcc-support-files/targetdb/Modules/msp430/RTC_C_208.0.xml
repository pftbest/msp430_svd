            <bitenum id="RTCTEVIFG" value="0x6" description="Interrupt Source: RTC interval timer; Interrupt Flag: RTCTEVIFG"/>
            <bitenum id="RTCAIFG" value="0x8" description="Interrupt Source: RTC user alarm; Interrupt Flag: RTCAIFG"/>
